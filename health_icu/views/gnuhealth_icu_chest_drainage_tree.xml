<odoo>
    <data>

        <record id='gnuhealth_icu_chest_drainage_view_tree' model='ir.ui.view'>
            <field name="name">ICU Test Drainage</field>
            <field name="model">gnuhealth.icu.chest.drainage</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="location" expand="1"/>
                    <field name="suction"/>
                    <field name="suction_pressure"/>
                    <field name="fluid_volume"/>
                    <field name="fluid_aspect"/>
                    <field name="oscillation"/>
                    <field name="air_leak"/>
                    <field name="remarks" expand="1"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
