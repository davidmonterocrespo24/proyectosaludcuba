<odoo>

    <record model="ir.ui.view" id="gnuhealth_patient_view_form_extend">
        <field name="name">gnuhealth.patient.view.form.extend</field>
        <field name="model">gnuhealth.patient</field>
        <field name="inherit_id" ref="medical.gnuhealth_patient_view_form"/>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name=rh]" position="after">
                <label name="patient_status"/>
                <field name="patient_status"/>
            </xpath>
        </field>
    </record>
</odoo>


