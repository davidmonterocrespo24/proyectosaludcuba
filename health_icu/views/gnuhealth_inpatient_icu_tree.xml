<odoo>
    <data>
        <record id='gnuhealth_inpatient_icu_view_tree' model='ir.ui.view'>
            <field name="name">Inpatient ICU</field>
            <field name="model">gnuhealth.inpatient.icu</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="admitted" expand="1"/>
                    <field name="icu_admission_date" widget="date"/>
                    <field name="discharged_from_icu"/>
                    <field name="icu_discharge_date" widget="date"/>
                    <field name="icu_stay"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
