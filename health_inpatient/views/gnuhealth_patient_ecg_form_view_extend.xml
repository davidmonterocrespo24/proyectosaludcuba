<odoo>

    <record model="ir.ui.view" id="gnuhealth_patient_ecg_form_extend">
        <field name="name">gnuhealth.patient.ecg.form.extend</field>
        <field name="model">gnuhealth.patient.ecg</field>
        <field name="inherit_id" ref="health.gnuhealth_patient_ecg_form"/>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">
            <xpath expr="/form/field[@name=name]" position="after">
                <label name="inpatient_registration_code"/>
                <field name="inpatient_registration_code"/>
            </xpath>
        </field>
    </record>
</odoo>
