<odoo>

    <record model="ir.ui.view" id="medical_appointment_form_view_extend">
        <field name="name">medical.appointment.form.view.extend</field>
        <field name="model">medical.appointment</field>
        <field name="inherit_id" ref="medical.medical_appointment_form"/>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">
            <xpath expr="/form/group/field[@name=institution]" position="after">
                <label name="inpatient_registration_code"/>
                <field name="inpatient_registration_code"/>
            </xpath>
        </field>
    </record>
</odoo>

