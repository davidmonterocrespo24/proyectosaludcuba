<odoo>

    <record model="ir.ui.view" id="gnuhealth_patient_tree_view_extend">
        <field name="name">gnuhealth.patient.tree.view.extend</field>
        <field name="model">gnuhealth.patient</field>
        <field name="inherit_id" ref="health.gnuhealth_patient_tree"/>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">
            <xpath expr="/tree/field[@name=puid]" position="after">
                <field name="patient_status"/>
            </xpath>
        </field>
    </record>
</odoo>

